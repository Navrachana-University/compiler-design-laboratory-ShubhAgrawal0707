સંખ્યા i;
i = 0;

માટે (i = 0; i < 3; i = i + 1) {
    જો (i == 1) {
        દરશાવો i;
    } નહીંતર {
        दरशાવો "અન્ય";
    }
}

સ્વિચ (i) {
    case 0:
        દરશાવો "શૂન્ય";
        break;
    case 1:
        दरशાવો "એક";
        break;
    default:
        दरशાવો "અજ્ઞાત";
}
